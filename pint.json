{
    "preset": "laravel",
    "exclude": [
        "storage",
        "vendor"
    ],
    "rules": {
        "array_syntax": {
            "syntax": "short"
        },
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "no_unused_imports": true,
        "single_quote": true,
        "trailing_comma_in_multiline": true,
        "no_extra_blank_lines": true,
        "array_indentation": true,
        "no_multiline_whitespace_around_double_arrow": true
    }
}
